using System;
using System.CodeDom;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace обучение
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int filledRows;
            int extraPictures;

            int pictures = 52;
            int picturesInRow = 3;
          
            extraPictures = pictures % picturesInRow;
            filledRows = pictures / picturesInRow;

            Console.WriteLine($"Можно вывести {filledRows} заполненых рядов. {extraPictures} картинка будет сверх меры.");
        }
    }
}
