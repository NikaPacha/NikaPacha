using System;
using System.Collections.Generic;
using System.Linq;
using System.Runtime.CompilerServices;
using System.Text;
using System.Threading.Tasks;
namespace обучение
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int interval = 7;
            int firstNumber = 5;
            int lastNumber = 96;

            //for - так как нам известны точные ганици цикла
            for (int i = firstNumber; i <= lastNumber; i += interval)
            {
                Console.WriteLine(i);
            }
        }
    }
}

